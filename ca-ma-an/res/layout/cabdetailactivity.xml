<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/RelativeLayout01" android:layout_height="fill_parent"
	android:layout_width="fill_parent" android:background="@drawable/border">
	<RelativeLayout android:id="@+id/RelativeLayout02"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<ImageView android:layout_below="@+id/RelativeLayout02"
			android:id="@+id/ImageView01" android:layout_width="90px"
			android:layout_height="90px" android:background="@drawable/img"
			android:layout_marginLeft="27px" android:layout_marginTop="10px"></ImageView>
	</RelativeLayout>
	<TextView android:layout_toRightOf="@+id/RelativeLayout02"
		android:id="@+id/TextView01" android:text="@+id/TextView01"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textColor="#000000" android:layout_marginTop="25px" android:layout_marginLeft="20px"></TextView>
	<TextView android:id="@+id/TextView02" android:text="@+id/TextView02"
		android:layout_below="@+id/RelativeLayout02" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#000000"
		android:layout_marginTop="4px" android:layout_marginLeft="55px"></TextView>
	<TextView android:id="@+id/TextView03" android:text="@+id/TextView03"
		android:layout_below="@+id/TextView02" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#000000"
		android:layout_marginTop="4px" android:layout_marginLeft="55px"></TextView>
	<TextView android:id="@+id/TextView04" android:text="@+id/TextView04"
		android:layout_below="@+id/TextView03" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#000000"
		android:layout_marginTop="4px" android:layout_marginLeft="55px"></TextView>
	<TextView android:id="@+id/TextView05" android:text="@+id/TextView05"
		android:layout_below="@+id/TextView04" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#000000"
		android:layout_marginTop="4px" android:layout_marginLeft="55px"></TextView>

	<TextView android:id="@+id/TextView06" android:layout_below="@+id/TextView05"
		android:layout_height="wrap_content" android:textColor="#000000"
		android:text="Rating" android:layout_width="50px"
		android:layout_marginTop="4px" android:layout_marginLeft="55px"></TextView>
	<ImageView android:layout_toRightOf="@+id/TextView06"
		android:layout_below="@+id/TextView05" android:id="@+id/ImageView02"
		android:layout_width="32px" android:layout_height="32px"
		android:background="@drawable/str"></ImageView>

	<ImageView android:layout_toRightOf="@+id/ImageView02"
		android:layout_below="@+id/TextView05" android:id="@+id/ImageView03"
		android:layout_height="32px" android:layout_width="32px"
		android:background="@drawable/str"></ImageView>
	<ImageView android:layout_toRightOf="@+id/ImageView03"
		android:layout_below="@+id/TextView05" android:id="@+id/ImageView04"
		android:layout_height="32px" android:layout_width="32px"
		android:background="@drawable/str"></ImageView>
	<ImageView android:layout_toRightOf="@+id/ImageView04"
		android:layout_below="@+id/TextView05" android:id="@+id/ImageView05"
		android:layout_height="32px" android:layout_width="32px"
		android:background="@drawable/str"></ImageView>
	<ImageView android:layout_toRightOf="@+id/ImageView05"
		android:layout_below="@+id/TextView05" android:id="@+id/ImageView06"
		android:layout_height="32px" android:layout_width="32px"
		android:background="@drawable/str"></ImageView>

	<RelativeLayout android:id="@+id/RelativeLayout03"
		android:layout_below="@+id/TextView06" android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<Button android:id="@+id/Button01" android:layout_below="@+id/ImageView02"
			android:layout_width="130px" android:layout_height="30px"
			android:background="@drawable/cilck" android:layout_marginTop="10px"
			android:layout_marginLeft="85px"></Button>
	</RelativeLayout>

</RelativeLayout>

